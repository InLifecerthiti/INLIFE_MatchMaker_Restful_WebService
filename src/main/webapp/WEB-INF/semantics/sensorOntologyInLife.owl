    <rdfs:label xml:lang="it">è caratterizzato da</rdfs:label>
    <rdfs:domain rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Collection"/>
    <rdfs:range rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Concept"/>
