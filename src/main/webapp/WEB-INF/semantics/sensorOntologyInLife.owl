          <owl:Restriction>
            <owl:someValuesFrom>
              <owl:Class rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#Collective"/>
            </owl:someValuesFrom>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#isMemberOf"/>
            </owl:onProperty>
