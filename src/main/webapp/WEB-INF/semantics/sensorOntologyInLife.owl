      <owl:ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#hasRegion"/>
    </rdfs:subPropertyOf>
    <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Base#Time</rdfs:seeAlso>
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#introduces">
