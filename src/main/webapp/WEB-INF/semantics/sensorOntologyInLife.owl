    <rdfs:domain rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Concept"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#directlyFollows"/>
