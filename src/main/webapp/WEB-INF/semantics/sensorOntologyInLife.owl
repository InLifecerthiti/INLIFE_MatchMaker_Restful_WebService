      <owl:ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#isConcretelyExpressedBy"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/ssnx/ssn#attachedSystem">
