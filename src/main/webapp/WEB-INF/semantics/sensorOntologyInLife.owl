    <rdfs:range rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#InformationRealization"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#isParametrizedBy">
